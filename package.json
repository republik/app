{
  "name": "republikapp",
  "version": "2.2.3",
  "license": "bsd-3-clause",
  "scripts": {
    "android": "ENVFILE=.env.development react-native run-android",
    "android-staging": "ENVFILE=.env.staging react-native run-android",
    "android-production": "ENVFILE=.env.production react-native run-android",
    "ios": "react-native run-ios --scheme 'republikapp'",
    "ios-staging": "react-native run-ios --scheme 'republikapp (staging)'",
    "ios-production": "react-native run-ios --scheme 'republikapp (production)'",
    "deploy-ios-staging": "cd ./ios && fastlane ios staging",
    "deploy-ios-production": "cd ./ios && fastlane ios production",
    "deploy-android-staging": "cd ./android && ENVFILE=.env.staging fastlane staging",
    "deploy-android-production": "cd ./android && ENVFILE=.env.production fastlane production",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "version": "react-native-version --skip-tag",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-cookies/cookies": "^6.1.0",
    "events": "^3.3.0",
    "react": "17.0.2",
    "react-native": "0.68.6",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.2.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-haptic-feedback": "^1.14.0",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-notifications": "^4.3.3",
    "react-native-safe-area-context": "4.1.4",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-track-player": "^3.2.0",
    "react-native-webview": "^11.23.1",
    "url": "^0.11.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/events": "^3.0.0",
    "@types/react-native": "^0.68.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "isSwift": true
}
